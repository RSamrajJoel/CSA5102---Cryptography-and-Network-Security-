#include <stdio.h>
#include <string.h>

int main() {
    char subkeys[16][49] = {
        "000110110000001011101111111111000111000001110010",
        "011110011010111011011001110110111100100111100101",
        "010101011111110010001010010000101100111110011001",
        "011100101010110111010110110110110011010100011101",
        "011111001110110000000111111010110101001110101000",
        "011000111010010100111110010100000111101100101111",
        "111011001000010010110111111101100001100010111100",
        "111101111000101000111010110000010011101111111011",
        "111000001101101111101011111011011110011110000001",
        "101100011111001101000111101110100100011001001111",
        "010011010011110111101101101001110110111100100111",
        "011111001110110110100111011010111111010110111010",
        "100111101110111111111101110100111100101111111110",
        "001110010111100000101001101101101011011110110001",
        "100101111100010111010001111110101011101001000001",
        "010111110100001110110111111100101110011100111010"
    };

    printf("First 24 bits of each DES subkey:\n\n");
    for (int i = 0; i < 16; i++) {
        char first24[25];
        strncpy(first24, subkeys[i], 24);
        first24[24] = '\0';
        printf("Subkey %2d: %s\n", i + 1, first24);
    }

    return 0;
}
